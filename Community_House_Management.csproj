    <Resource Include="Resources\Images\menuicon.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Resources\Images\plusicon.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
