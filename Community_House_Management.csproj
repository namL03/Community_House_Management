    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Resources\Images\back_ground.jpg" />
    <None Remove="Resources\Images\back_ground_3.jpg" />
    <None Remove="Resources\Images\category.png" />
    <None Remove="Resources\Images\menuicon.png" />
    <None Remove="Resources\Images\pepe.jpg" />
    <None Remove="Resources\Images\plusicon.png" />
